#include <stdio.h>

int main() {
    int N;
    scanf("%d", &N);
    
    int nums[N];
    for (int i = 0; i < N; i++) {
        scanf("%d", &nums[i]);
    }
    
    int result = 0;
    for (int i = 0; i < N; i++) {
        result ^= nums[i];
    }
    
    printf("%d\n", result);
    
    return 0;
}
